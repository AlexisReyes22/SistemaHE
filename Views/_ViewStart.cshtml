@{
    if (Session["Rol"].Equals("Nolog") || Session["Rol"].Equals(null))
    {
        Layout = "~/Views/Shared/_Layout.cshtml";

    }
    else if (Session["Rol"].Equals("Director"))
    {
        Layout = "~/Views/Shared/_Layout _ Director.cshtml";

    }
    else if (Session["Rol"].Equals("Jefe"))
    {
        Layout = "~/Views/Shared/_Layout _ Jefe.cshtml";

    }
    else if (Session["Rol"].Equals("Admin"))
    {
        Layout = "~/Views/Shared/_Layout _ Admin.cshtml";

    }
    else
    {
        Layout = "~/Views/Shared/_Layout _ Funcionario.cshtml";

    }


}




