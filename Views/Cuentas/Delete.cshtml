@model SistemaHE.Models.Cuentas

@{
    ViewBag.Title = "Delete";
}

<div id="textoH">
    <br />
    <h2>Cuentas</h2>
</div>

<div>
    <div id="ContentL">
        <div class="ContenidoL">
            <h3>¿Está seguro que desea eliminar esto?</h3>
            <div class="row">
                <div id="formula" class="col s6">


                    @Html.LabelFor(model => model.Contrasenna, "Contraseña", htmlAttributes: new { @class = "central-align", @style = "" })



                    @Html.EditorFor(model => model.Contrasenna, new { htmlAttributes = new { @class = "black-text", disabled = "disabled" } })

                    @Html.LabelFor(model => model.Usuarios.Nombre_Completo, "Nombre Completo", htmlAttributes: new { @class = "central-align" })


                    @Html.EditorFor(model => model.Usuarios.Nombre_Completo, new { htmlAttributes = new { @class = "black-text",  disabled = "disabled" } })
                </div>
            </div>

            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div id="pagina">

                    <input type="submit" value="Eliminar" class="waves-button-input btn-small  " />
                    @Html.ActionLink("Volver", "Index", null, htmlAttributes: new { @class = "waves-button-input btn-small  " })

                </div>
            }


        </div>
    </div>
</div>
