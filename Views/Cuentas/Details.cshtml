@model SistemaHE.Models.Cuentas

@{
    ViewBag.Title = "Details";
}

<div id="textoH">
    <br />
    <h2>Cuentas</h2>
</div>

<div>

    <div id="ContentL">
        <div class="ContenidoL">
            <h3>Detalles</h3>

            <div class="row">


                <div id="formula" class="col l6">
                    @Html.LabelFor(model => model.Usuarios.Nombre_Completo, "Nombre Completo")
                    @Html.EditorFor(model => model.Usuarios.Nombre_Completo, new { htmlAttributes = new { @class = "", disabled = "disabled" } })
                  
                    @Html.LabelFor(model => model.Identificacion, "Contraseña")
                    @Html.EditorFor(model => model.Contrasenna, new { htmlAttributes = new { @class = "", disabled = "disabled" } })

                </div>


            </div>

            <p id="pagina">
                @Html.ActionLink("Editar", "Edit", new { id = Model.ID_Cuenta }, htmlAttributes: new { @class = "waves-button-input btn-small light-blue darken-2  white-text" }) |
                @Html.ActionLink("Volver", "Index", null, htmlAttributes: new { @class = "waves-button-input btn-small light-blue darken-2  white-text" })
            </p>

        </div>
    </div>

</div>

