@model SistemaHE.Models.Departamentos

@{
    ViewBag.Title = "Details";
}
<div id="textoH">
    <br />
    <h2>Departamentos</h2>
</div>




<div id="ContentL">
    <div class="ContenidoL">
        <h3>Detalles</h3>
       
        <div class="row">
            <div id="formula" class="col s6">
                @Html.LabelFor(model => model.Nombre_Departamento, "Departamento")

                @Html.EditorFor(model => model.Nombre_Departamento, new { htmlAttributes = new { @class = "", disabled = "disabled" } })

            </div>

        </div>
        <div id="pagina">
            @Html.ActionLink("Editar", "Edit", new { id = Model.ID_Departamento }, htmlAttributes: new { @class = "waves-button-input btn-small " })
            @Html.ActionLink("Volver", "Index", null, htmlAttributes: new { @class = "waves-button-input btn-small " })
        </div>
    </div>
</div>

